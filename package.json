{
  "name": "sucka",
  "version": "0.1.0",
  "author": "Jonathon Morgan <jonathon@ushahidi.com>",
  "description": "A framework for sucking data into CrisisNet",
  "scripts": {
    "dev-server": "NODE_ENV=development nodemon app/main.js",
    "staging-server": "NODE_ENV=staging node app/main.js",
    "prod-server": "NODE_ENV=production node app.js",
    "run-test": "NODE_ENV=test mocha",
    "setup-sources": "NODE_ENV=development node bin/setup-sources.js",
    "make-docs": "doxx --template ./docs-template.jade --title \"Hey, Sucka\" --source ./app --target ../sucka-docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ushahidi/sucka.git"
  },
  "dependencies": {
    "nodemon": "0.7.10",
    "underscore": "1.5.1",
    "request": "~2.25.0",
    "kue": "0.7.5",
    "require-all": "0.0.8",
    "promise": "~4.0.0",
    "moment": "~2.5.1",
    "twit": "~1.1.12",
    "config": "~0.4.35",
    "redis": "~0.10.0",
    "mongoose": "~3.8.7",
    "mongoose-validator": "~0.2.2",
    "underscore.string": "~2.3.3",
    "winston": "~0.7.2",
    "async": "~0.2.10",
    "simple-redis-queue": "~0.9.3",
    "fbgraph": "~0.2.9",
    "fb": "~0.6.2",
    "fast-csv": "0.2.2",
    "unzip": "~0.1.9",
    "validator": "~3.5.0"
  },
  "devDependencies": {
    "mocha": "1.13.0",
    "chai": "~1.9.0",
